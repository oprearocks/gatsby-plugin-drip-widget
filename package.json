{
  "name": "gatsby-plugin-drip",
  "description": "A Gatsby.js plugin that provides the drip script you can embed in any Gatsby.js website.",
  "main": "index.js",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "drip",
    "email",
    "marketing",
    "newsletter"
  ],
  "author": "Adrian Oprea <adrian@oprea.rocks> (https://oprea.rocks)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@babel/runtime": "^7.0.0-rc.3",
    "podcast": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0"
  },
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  }
}
